{
	"name": "@devnitinreddy/share-component-lib",
	"version": "1.0.0",
	"description": "Shared component library for multi-tenant UmiJS applications",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"publishConfig": {
		"access": "public"
	},
